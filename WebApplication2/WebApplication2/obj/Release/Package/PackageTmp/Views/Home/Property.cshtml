@using System.Data;
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8">
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Document</title>
    <script src="https://cdn.jsdelivr.net/npm/jquery@3.4.0/dist/jquery.min.js"></script>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/fancyapps/fancybox@3.5.7/dist/jquery.fancybox.min.css">
    <script src="https://cdn.jsdelivr.net/npm/fancyapps/fancybox@3.5.7/dist/jquery.fancybox.min.js"></script>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Home Property | Gallery</title>

    <!-- Favicon -->
    <link rel="shortcut icon" href="~/img/favicon.ico" type="image/x-icon">

    <!-- Font awesome -->
    <link href="~/css/font-awesome.css" rel="stylesheet">
    <!-- Bootstrap -->
    <link href="~/css/bootstrap.css" rel="stylesheet">
    <!-- slick slider -->
    <link rel="stylesheet" type="text/css" href="~/css/slick.css">
    <!-- price picker slider -->
    <link rel="stylesheet" type="text/css" href="~/css/nouislider.css">
    <!-- Fancybox slider -->
    <link rel="stylesheet" href="~/css/jquery.fancybox.css" type="text/css" media="screen" />
    <!-- Theme color -->
    <link id="switcher" href="~/css/theme-color/default-theme.css" rel="stylesheet">

    <!-- Main style sheet -->
    <link href="~/css/style.css" rel="stylesheet">


    <!-- Google Font -->
    <link href='https://fonts.googleapis.com/css?family=Vollkorn' rel='stylesheet' type='text/css'>
    <link href='https://fonts.googleapis.com/css?family=Open+Sans' rel='stylesheet' type='text/css'>


    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->

    <style>
        .column {
            float: left;
            width: 25%;
        }

            /* Style the images inside the grid */
            .column img {
                opacity: 0.8;
                cursor: pointer;
            }

                .column img:hover {
                    opacity: 1;
                }

        .propertyfeaturesgallery {
            float: left;
        }

        .row:after {
            content: "";
            display: table;
            clear: both;
        }

        .closebtn {
            position: relative;
            color: black;
            font-size: 35px;
            cursor: pointer;
        }
        /* Expanding image text */
        #imgtext {
            position: absolute;
            bottom: 15px;
            left: 15px;
            color: white;
            font-size: 20px;
        }

        .container {
            position: relative;
        }
    </style>
</head>

<body>

    <!-- Pre Loader -->
    <!-- END SCROLL TOP BUTTON -->
    <!-- Start menu section ---------------------------->
    <section id="aa-menu-area">

        <nav class="navbar navbar-default main-navbar" role="navigation">
            <div class="container">
                <div class="navbar-header">
                    <!-- FOR MOBILE VIEW COLLAPSED BUTTON -->
                    <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <!-- LOGO -->
                    <!-- Text based logo -->
                    <a class="navbar-brand aa-logo"> Memaar <span>AlMorshdy</span></a>
                    <!-- Image based logo -->
                    <!-- <a class="navbar-brand aa-logo-img" href="index.html"><img src="img/logo.png" alt="logo"></a> -->
                </div>

            </div>
        </nav>
    </section>
    <!-- end menu section -->
    <!-- Start header section --------------------------->
    <header id="aa-header">
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <div class="aa-header-area">
                        <div class="row">
                            <div class="col-md-6 col-sm-6 col-xs-6">
                                <div class="aa-header-left">
                                    <div class="aa-telephone-no">
                                        <span class="fa fa-phone"></span>
                                        19086
                                    </div>
                                    <div class="aa-email hidden-xs">
                                        <span class="fa fa-envelope-o"></span>  info@morshedy.com
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6 col-sm-6 col-xs-6">
                                <div class="aa-header-right">


                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </header>
    <!-- End header section -->
    <!-- Start Proerty header --------------------------->
    <section id="aa-property-header">
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <div class="aa-property-header-inner">
                        <h2>Zahra</h2>
                        <ol class="breadcrumb">
                            <li><a>HOME</a></li>
                            <li><a>Gallery</a></li>
                        </ol>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- End Proerty header  -->




    <section id="aa-properties">
        <div class="container">
            <div class="row">

                <div class="col-md-8">
                    <!-- Start properties content body -->
                    <div class="aa-properties-details">
                        @foreach (DataRow row in Model.Rows)
                        {
                            <h2>@row["unit_desc"] </h2>
                        }
                        <div class="aa-properties-details-img">


                            @foreach (DataRow row in Model.Rows)
                            {
                                <img src="~/img/test/@row["img_name1"]">
                                <img src="~/img/test/@row["img_name2"]">
                                if (@row["img_name3"] is 0)
                                {
                                    <img src="~/img/test/@row["img_name3"]">
                                }
                                if (!(@row["img_name4"] is 0))
                                {

                                    <img src="~/img/test/@row["img_name4"]">


                                }


                            }



                        </div>


                    </div>
                </div>
                <div class="col-md-4">
                    <h2>
                        Property Video
                    </h2>
                    <iframe width="560" height="530" src="https://www.youtube.com/embed/mn8btXKk0zg" title="YouTube video player" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>


                </div>
            </div>
            <div class="row">
                <div class="col-md-4">
                    <div class="aa-properties-details">

                        <div class="aa-properties-info">


                            <div class="row">

                                <h2 class="pp">Property Facilities</h2>
                                <ul>
                                    @foreach (DataRow row in Model.Rows)
                                    {
                                        <li>@row["f1"] </li>
                                        <li>@row["f2"] </li>
                                        <li>@row["f3"] </li>
                                        <li>@row["f4"] </li>
                                        <li>@row["f5"] </li>
                                        <li>@row["f6"] </li>
                                        <li>@row["f7"] </li>
                                        <li>@row["f8"] </li>
                                        <li>@row["f9"] </li>
                                        <li>@row["f10"] </li>


                                    }
                                </ul>

                                <div class="col-md-8">

                                </div>


                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-md-8">
                    <div style="text-align:center">
                        <h2 class="pp">Property Facilities Gallery</h2>
                    </div>

                    <!-- The four columns -->

                    <div class="column">
                        @foreach (DataRow row in Model.Rows)
                        {
                            <img src="~/img/test/@row["grid_6"]" alt="Nature" style="width:100%" onclick="myFunction(this);">
                            <img src="~/img/test/@row["grid_7"]" alt="Nature" style="width:100%" onclick="myFunction(this);">
                            <img src="~/img/test/@row["grid_1"]" alt="Nature" style="width:100%" onclick="myFunction(this);">

                        }
                    </div>

                    <div class="column">
                        @foreach (DataRow row in Model.Rows)
                        {
                            <img src="~/img/test/@row["grid_3"]" alt="Nature" style="width:100%" onclick="myFunction(this);">
                            <img src="~/img/test/@row["grid_9"]" alt="Nature" style="width:100%" onclick="myFunction(this);">
                            <img src="~/img/test/@row["grid_4"]" alt="Nature" style="width:100%" onclick="myFunction(this);">


                        }
                    </div>
                    <div class="column">
                        @foreach (DataRow row in Model.Rows)
                        {
                            <img src="~/img/test/@row["grid_8"]" alt="Nature" style="width:100%" onclick="myFunction(this);">
                            <img src="~/img/test/@row["grid_10"]" alt="Nature" style="width:100%" onclick="myFunction(this);">

                        }
                    </div>
                    <div class="column">
                        @foreach (DataRow row in Model.Rows)
                        {
                            <img src="~/img/test/@row["grid_2"]" alt="Nature" style="width:100%" onclick="myFunction(this);">
                            <img src="~/img/test/@row["grid_5"]" alt="Nature" style="width:100%" onclick="myFunction(this);">

                        }
                    </div>


                </div>

            </div>
            <div class="masterplan">
                <div class="container">
                    <span onclick="this.parentElement.style.display='none'" class="closebtn">&times;</span>
                    <img id="expandedImg" style="width:70%">
                </div>
                <h2>
                    Master Plan
                </h2>
                <a>
                    @foreach (DataRow row in Model.Rows)
                    {
                        <img src="~/img/test/@row["masterplan_image"]" width="100%" height="100%">
                    }

                </a>
            </div>

        </div>
    </section>


    <!-- Footer -->
    <footer id="aa-footer">
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <div class="aa-footer-area">
                        <div class="row">
                            <div class="col-md-6 col-sm-6 col-xs-12">
                                <div class="aa-footer-left">
                                    <p>Designed by <a rel="nofollow" href="https://emsegypt.net/">Egyptian Micro Solution</a></p>
                                </div>
                            </div>
                            <div class="col-md-6 col-sm-6 col-xs-12">
                                <div class="aa-footer-middle">
                                    <a href="https://www.facebook.com/EgyptEMS"><i class="fa fa-facebook"></i></a>
                                    <a href="http://www.emsegypt.net/"><i class="fa fa-globe"></i></a>
                                    <a href="mailto:samsung.swoperation@emsegypt.net"><i class="fa fa-envelope"></i></a>
                                    <a href="tel:01030146459"><i class="fa fa-mobile"></i></a>
                                </div>
                            </div>

                        </div>
                    </div>
                </div>
            </div>
        </div>
    </footer>
    <!-- / Footer -->




    <!-- jQuery library -->
    <!--<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script> -->
    <script src="~/js/jquery.min.js"></script>
    <!-- Include all compiled plugins (below), or include individual files as needed -->
    <script src="~/js/bootstrap.js"></script>
    <!-- slick slider -->
    <script type="text/javascript" src="~/js/slick.js"></script>
    <!-- Price picker slider -->
    <script type="text/javascript" src="~/js/nouislider.js"></script>
    <!-- mixit slider -->
    <script type="text/javascript" src="~/js/jquery.mixitup.js"></script>
    <!-- Add fancyBox -->
    <script type="text/javascript" src="~/js/jquery.fancybox.pack.js"></script>
    <!-- Custom js -->
    <script src="~/js/custom.js"></script>


    <script>
        function myFunction(imgs) {
            var expandImg = document.getElementById("expandedImg");
            expandImg.src = imgs.src;
            expandImg.parentElement.style.display = "block";

        }
    </script>
</body>

</html>
